<%- include("header.ejs") -%>
    <div class="box" id="heading">
      <h1> <%= ListTitle %> </h1>
    </div>
   <div class="box">
          <%  for(var i=0;i<newListItem.length;i++) {%>
            <div class="items">
              <input type="checkbox">
            <p>
              <%= newListItem[i] %>
            </p>
           </div>
           <% } %>
           <form class="item" action="/" method="post">
              <input type="text" name="newItem" placeholder="Next task" autocomplete="off">
              <button type="submit" name="list" value="<%= ListTitle %>">
                    +
              </button>
           </form>
     </div>
<%- include("footer.ejs") -%>
